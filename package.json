{
	"name": "detcord",
	"version": "1.0.2",
	"description": "Bulk delete your own Discord messages - Fast, secure, privacy-focused",
	"author": "Alex Howells <alex@welshwandering.com> (https://welshwandering.com)",
	"license": "MIT",
	"keywords": [
		"discord",
		"userscript",
		"tampermonkey",
		"greasemonkey",
		"violentmonkey",
		"bulk-delete",
		"message-delete",
		"privacy",
		"discord-messages",
		"discord-cleaner"
	],
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/welshwandering/detcord"
	},
	"scripts": {
		"build": "vite build",
		"build:userscript": "vite build --mode userscript",
		"dev": "vite",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@commitlint/cli": "^20.3.0",
		"@commitlint/config-conventional": "^19.0.0",
		"@types/node": "^25.0.3",
		"@vitest/coverage-v8": "^4.0.16",
		"husky": "^9.0.0",
		"jsdom": "^27.4.0",
		"typescript": "^5.7.0",
		"vite": "^7.3.0",
		"vite-plugin-banner": "^0.8.0",
		"vitest": "^4.0.16"
	},
	"engines": {
		"node": ">=18"
	}
}
